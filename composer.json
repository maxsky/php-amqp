{
    "name": "maxsky/php-amqp",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Sky",
            "email": "max940213@live.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MaxSky\\AMQP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1|^8.0",
        "ext-json": "*",
        "nesbot/carbon": "^2.72",
        "php-amqplib/php-amqplib": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0|^8.0|^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
